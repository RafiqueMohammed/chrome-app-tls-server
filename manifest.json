{
  "manifest_version": 2,
  "name": "Chrome app tls enabled http server",
  "version": "0.0.1",
  "minimum_chrome_version": "41",
  "app": {
    "background": {
      "scripts": [
        "tls-forge.js",
        "server-browserify.js"
